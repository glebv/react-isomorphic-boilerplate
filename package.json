{
  "name": "isomorphic-demo",
  "version": "0.0.1",
  "description": "A boilerplate for isomorphic React apps",
  "main": "lib/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib --experimental"
  },
  "keywords": [
    "isomorphic",
    "react",
    "app"
  ],
  "author": "Gleb Vinnikov",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-core": "^5.5.6",
    "babel-loader": "^5.1.4",
    "bootstrap": "^3.3.5",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.8",
    "bootstrap-webpack": "0.0.3",
    "css-loader": "^0.16.0",
    "errorhandler": "^1.4.2",
    "express": "^4.12.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "http-proxy": "^1.11.1",
    "imports-loader": "^0.6.4",
    "jade": "^1.10.0",
    "jquery": "^2.1.4",
    "json5": "^0.4.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.7",
    "react-router": "^0.13.3",
    "sass-loader": "^2.0.1",
    "serve-static": "^1.10.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "nodemon": "^1.3.7",
    "url-loader": "^0.5.6"
  }
}
